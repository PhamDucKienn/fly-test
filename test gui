
local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/PhamDucKienn/Gui/refs/heads/main/Gui"))()

local Window = OrionLib:MakeWindow({ Name = "Skid Hub V1.25(Beta)⚡", HidePremium = false, IntroText = "Skid Hub⚡", SaveConfig = true, ConfigFolder = "OrionTest" })

_G.AutoTap = true
_G.HighSpeed = true
_G.HighJump = true

function highjump()
    while _G.HighJump == true do
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.JumpPower = 150
        wait(1)
    end
end

function highspeed()
    while _G.HighSpeed == true do
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.WalkSpeed = 100
        wait(1)
    end
end

function autoclick()
    while _G.AutoTap == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("AddSpeed"):FireServer()
        wait(0.0000000000001)
    end
end

local FarmTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

FarmTab:AddToggle({
    Name = "Auto Clicker",
    Default = false,
    Callback = function(Value)
        _G.AutoTap = Value
        autoclick()
    end
})

FarmTab:AddToggle({
    Name = "High Speed",
    Default = false,
    Callback = function(Value)
        _G.HighSpeed = Value
        highspeed()
    end    
})

FarmTab:AddToggle({
	Name = "High Jump",
	Default = false,
	Callback = function(Value)
		_G.HighJump = Value
        highjump()
	end    
})

FarmTab:AddSlider({
            Name = "Slider"
            Min = 0,
            Max = 100,
            Default = 20,
            Color = Color3.fromRGB(255,255,255),
            Increment = 1,
            ValueName = "bananas",
            Callback = function(Value)
                   print(Value)
            end
})
